    <main>
        <section id="create-container">
            <div class="create-container-info">
                <h1>Create Volcano Wiki</h1>
                <form method="POST">
                    <label>Name:</label>
                    <input type="text" id="name" name="name" value="{{name}}" placeholder="...">
                    <label>Location:</label>
                    <input type="text" id="location" name="location" value="{{location}}" placeholder="...">
                    <label>Elevation:</label>
                    <input type="number" id="elevations" name="elevation" value="{{elevation}}" placeholder="...">
                    <label>Year of Last Eruption:</label>
                    <input type="number" id="last-year-eruption" name="lastEruption" value="{{lastEruption}}" placeholder="...">
                    <label>Image:</label>
                    <input type="text" id="image" name="imageUrl" value="{{imageUrl}}" placeholder="http://...">
                    <label>Volcano Type:</label>
                    <select id="type" name="typeVolcano">
                    {{#each options}}
                        <option value="{{value}}" {{#if selected}}selected{{/if}} >{{title}}</option>
                    {{/each}}
                    </select>
                    <label>Description:</label>
                    <textarea id="description" name="description" placeholder="...">{{description}}</textarea>
                    <input type="submit" id="btn" value="Submit"></input>
                </form>
            </div>
        </section>
    </main>
